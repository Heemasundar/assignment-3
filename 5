#include <stdio.h>
#include <string.h>

#define MAX_SIZE 100

int main() {
    char str[MAX_SIZE];
    int freq[256] = {0}, max = 0;
    char maxChar;

    printf("Enter a string: ");
    fgets(str, MAX_SIZE, stdin);

    // find frequency of each character
    for (int i = 0; str[i] != '\0'; i++) {
        freq[str[i]]++;
    }

    // find character with maximum frequency
    for (int i = 0; i < 256; i++) {
        if (freq[i] > max) {
            max = freq[i];
            maxChar = (char)i;
        }
    }

    printf("Max repeated character in the string = %c\n", maxChar);
    printf("It occurs %d times", max);

    return 0;
}
