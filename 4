#include <stdio.h>

int gcd(int a, int b);

int main() {
    int a, b, lcm, max;

    printf("Enter two positive integers: ");
    scanf("%d %d", &a, &b);

    // find maximum value between a and b
    max = (a > b) ? a : b;

    while (1) {
        if (max % a == 0 && max % b == 0) {
            lcm = max;
            break;
        }
        ++max;
    }

    printf("LCM of %d and %d is %d.", a, b, lcm);

    return 0;
}
